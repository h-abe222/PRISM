{
  "name": "prism-investment",
  "version": "1.0.0",
  "description": "PRISM - Private Real-estate Investment Smart Matching",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "next build && next export -o dist",
    "start": "next start",
    "lint": "next lint && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "next lint --fix && eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "cz",
    "release": "standard-version",
    "deploy:staging": "vercel --prod",
    "deploy:production": "vercel --prod --scope=prism",
    "lighthouse": "lighthouse https://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "security:check": "npm audit --audit-level=moderate"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3",
    "@tanstack/react-query": "^5.12.2",
    "axios": "^1.6.2",
    "zustand": "^4.4.7",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "@hookform/resolvers": "^3.3.2",
    "tailwindcss": "^3.3.6",
    "framer-motion": "^10.16.16",
    "recharts": "^2.10.3",
    "date-fns": "^3.0.6",
    "react-hot-toast": "^2.4.1",
    "clsx": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "@prisma/client": "^5.7.1",
    "sharp": "^0.33.1",
    "@vercel/analytics": "^1.1.1",
    "@sentry/nextjs": "^7.91.0",
    "react-map-gl": "^7.1.6",
    "mapbox-gl": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@storybook/react": "^7.6.5",
    "@storybook/nextjs": "^7.6.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "@next/bundle-analyzer": "^14.0.4",
    "lighthouse": "^11.4.0",
    "vercel": "^32.7.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franchette-corp/prism-investment.git"
  },
  "keywords": [
    "real-estate",
    "investment",
    "nextjs",
    "react",
    "typescript",
    "prism"
  ],
  "author": "Franchette Corp.",
  "license": "PROPRIETARY",
  "bugs": {
    "url": "https://github.com/franchette-corp/prism-investment/issues"
  },
  "homepage": "https://prism-investment.com"
}